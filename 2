import numpy as np
import matplotlib.pyplot as plt

x = np.linspace(-10, 10, 100)


fig, ax = plt.subplots(2, 2, figsize=(10, 8))

ax[0, 0].plot(x, np.cos(x))
ax[0, 0].set_title('cos(x)')

ax[0, 1].plot(x, np.sin(x))
ax[0, 1].set_title('sin(x)')

ax[1, 0].plot(x, x**2)
ax[1, 0].set_title('x^2')

ax[1, 1].plot(x, 2/x)
ax[1, 1].set_title('2/x')

fig.suptitle('Графики функций')

plt.show()
